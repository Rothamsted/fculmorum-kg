<?xml version = "1.0" encoding = "UTF-8" ?>
<parser 
	xmlns = "http://www.ondex.org/xml/schema/tab_parser"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<delimiter>\t</delimiter>
	<quote>"</quote>
	<encoding>UTF-8</encoding>
	<start-line>1</start-line>
		
	<concept id="protein">
		<class>Protein</class>
		<data-source>ENSEMBL</data-source>
		<name preferred="true">
			<column index='1' />
		</name>
		<accession data-source="ENSEMBL-FUNGI">
			<column index='0' />
		</accession>
		<attribute type="TEXT" name="Protein_size">
			<column index='2' />
		</attribute>
		<attribute type="TEXT" name="annotation	">
			<column index='3' />
		</attribute>
	</concept>
</parser>